<%- if user_signed_in? -%>
  <li>
    <%= link_to 'Logout', destroy_user_session_path, method: :delete, data:{toggle: "tooltip", placement:"right"}, title:"Logout" %>
  </li>
<%- else -%>
  <li>
    <%= link_to 'Log in', new_user_session_path, data:{toggle: "tooltip", placement:"right"}, title:"Log in" %>
  </li>
  <li>
    <%= link_to 'Sign up', new_user_registration_path, data:{toggle: "tooltip", placement:"right"}, title:"Sign up" %>
  </li>
<%- end -%>
