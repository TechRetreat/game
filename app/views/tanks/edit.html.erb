<div id="replay-notices">
  <div id="toolbar">
    <span id="unread">0</span>
    TechTanks Console
    <a id="clear-console">Clear</a>
    <a id="switch-sides">Switch Sides</a>
  </div>
  <div id="console"></div>
</div>
<div class="row" id="main">
  <div class="editor-screen nopadding" id="left">
    <%= render 'tanks/editor' %>
  </div>
  <div class="sidepane-screen nopadding" id="right">
    <div id="handle"></div>
    <%= render 'tanks/sidepane' %>
  </div>
</div>

<%= javascript_include_tag 'edit', 'data-turbolinks-track' => true %>
